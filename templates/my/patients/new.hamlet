<header.app-top-app-bar.background>
  <div #headerStart>
    <md-icon-button href=@{MyPatientsR did} aria-label=_{MsgBack}>
      <md-icon>arrow_back
          
    <h1.title-large>
      _{MsgPatients}
      
<main>

  ^{widgetBanner msgs}

  <form method=post action=@{MyPatientsR did} enctype=#{et} novalidate>
    ^{fw}
    <div.app-form-actions>
      <md-text-button type=button href=@{MyPatientsR did}>
        _{MsgCancel}
      <md-filled-button type=submit>
        _{MsgSave}

^{widgetSnackbar msgs}
